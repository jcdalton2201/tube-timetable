{
  "name": "tube-timetable",
  "version": "0.0.1",
  "description": "Split card example with London Tube",
  "main": "index.js",
  "scripts": {
    "cc": "node scripts/createComponent.js",
    "copyapp": "cpx 'web/**/*.*' docs && cpx 'dist/**/*.js' docs",
    "build:deploy": "rollup -c --environment BUILD:production",
    "prebuild": "cpx 'src/routes/tube-app/lineFetch.js' dist/routes/tube-app && cpx 'src/routes/tube-app/arrivalFetch.js' dist/routes/tube-app",
    "build": "rollup -c --environment BUILD:dev",
    "docker": "node scripts/dockerBuild",
    "docker-run": "node scripts/dockerStart",
    "predeploy": "npm run build && workbox generateSW ./workbox-config.js",
    "deploy": " npm run copyapp",
    "prestart": "npm run build",
    "prestart:deploy": "npm run deploy",
    "start": "browser-sync start -c ./bs-config.js & npm run watchStyle",
    "start:deploy": "browser-sync start -c ./docs-config.js & npm run watchStyle",
    "test": "karma start spec/karma.config.js",
    "watchStyle": "watch 'npm run build' src/",
    "watchDeploy": "watch 'npm run deplay' src/",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jcdalton2201/tube-timetable"
  },
  "keywords": [
    "Tube",
    "split card",
    "web"
  ],
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "glob-fs": "^0.1.7",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-firefox-launcher": "^1.2.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-rollup-preprocessor": "^7.0.2",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.13.1",
    "proxy-middleware": "^0.15.0",
    "replacestream": "^4.0.3",
    "rollup": "^1.20.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "sonarqube-scanner": "^2.9.1",
    "standard-version": "^8.0.1",
    "watch": "^1.0.2",
    "workbox-cli": "^3.6.3"
  },
  "author": "Jason Dalton",
  "license": "MIT",
  "dependencies": {
    "babel-jest": "^24.9.0",
    "cpx": "^1.5.0",
    "docker-cmd-js": "^4.1.0",
    "eslint": "^6.2.2",
    "jasmine-core": "^3.4.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "set-value": "^4.0.1",
    "terser": "^4.2.1",
    "uglify-es": "^3.3.9"
  }
}
